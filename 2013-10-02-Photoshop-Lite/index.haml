    %title Template
    %link{rel: 'stylesheet', href: 'css/normalize.css'}
    %link{rel: 'stylesheet', href: 'css/style.css'}
  %body
    %h1 Photoshop Lite
    %input#color{type: 'text', autofocus: true, placeholder: '#ffcc33'}
